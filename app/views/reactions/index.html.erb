<h1>Reactions#index</h1>
<p>Find me in app/views/reactions/index.html.erb</p>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <% @reactions.each do |reaction| %>
        <tr>
          <td><%= reaction.id %></td>
          <td><%= reaction.name %></td>
          <td><%= link_to 'Show', reaction_path(reaction.id) %></td>
          <td><%= link_to 'Edit', edit_reaction_path(reaction.id) %></td>
          <td><%= link_to 'Destroy', reaction_path(reaction.id), method: :delete, "data-confirm" => "Are you sure?"%></td>
        </tr>
      <%end %>
  </tbody>
</table>