<% count = UserReaction.all.count %>
hoge: <%= count %>
  <% reactions.each do |reaction| %>
    <% size = 50 + 50 * (((reaction.user_reactions.count * 100).to_f / count) / 33) %>
    <%= image_tag reaction.get_image(size, size) %>
    size:
    <%= size.to_i %>
  <% end %>
